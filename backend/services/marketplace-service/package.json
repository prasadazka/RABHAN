{
  "name": "@rabhan/marketplace-service",
  "version": "1.0.0",
  "description": "RABHAN Solar BNPL Platform - Marketplace Service (SAMA Compliant)",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build:prod": "NODE_ENV=production tsc --build",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node --enable-source-maps dist/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node -r tsconfig-paths/register scripts/run-migrations.ts",
    "migrate:status": "ts-node -r tsconfig-paths/register scripts/run-migrations.ts status",
    "migrate:rollback": "ts-node -r tsconfig-paths/register scripts/run-migrations.ts rollback"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.20",
    "@types/pg": "^8.11.2",
    "@types/redis": "^4.0.11",
    "@types/winston": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pg-types": "^4.1.0",
    "redis": "^4.6.13",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "rabhan",
    "solar",
    "bnpl",
    "marketplace",
    "sama-compliant",
    "saudi-arabia",
    "fintech"
  ],
  "author": "RABHAN Development Team",
  "license": "PROPRIETARY"
}
